
<h1>Listing Users</h1>
 
<table>
  <thead>
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>
 
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.first_name %></td>
        <td><%= user.last_name %></td>    
        <td><%= user.email %></td>    
      </tr>
    <% end %>
  </tbody>
</table>
 
<br>
 
<%= link_to "New user", users_path %>